{
  "expo": {
    "name": "WMORY",
    "slug": "photoapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "photoappfrontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.photoapp.project",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.475359737954-6d67avriu75rq2rsqc92so3tms7sitpv"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png", 
        "backgroundColor": "#3f3f3f" 
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.photoapp.project",
      "googleServicesFile": "./google-services.json",
      "androidStatusBar": {
        "barStyle": "light-content",
        "backgroundColor": "#00000000",
        "translucent": true
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/app-check",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Uygulamanın fotoğraf çekebilmesi için kamera izni gerekiyor.",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "Video kaydı için mikrofon izni gerekiyor."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2f568aac-fb08-49cd-ad8c-3006c61902e5"
      }
    },
    "owner": "furkankcodes"
  }
}
